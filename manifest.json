{
  "name": "FBlock",
  "version": "1.0",
  "description": "web debugger",
  "permissions": ["webRequest", "webRequestBlocking", "storage", "<all_urls>"],
  "background": {
    "scripts": ["./static/js/actions.js", "background.js"]
  },
  "browser_action": {
    "default_icon": "./static/images/icon.png"
  },
  "manifest_version": 2
}
